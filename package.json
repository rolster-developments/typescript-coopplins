{
  "name": "@rolster/typescript-coopplins",
  "version": "4.1.2",
  "description": "Package that contains a working environment to develop web applications and API's.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "unpkg": "./dist/es/index.js",
  "types": "./dist/esm/index.d.ts",
  "license": "MIT",
  "author": "Rolster Technology <rolster.developments@gmail.com>",
  "contributors": [
    {
      "name": "Daniel Andr√©s Castillo Pedroza",
      "email": "ingdacastillop@gmail.com"
    }
  ],
  "files": [
    "dist/",
    "types/"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./types": {
      "types": "./dist/esm/types/index.d.ts",
      "node": "./dist/cjs/types/index.js",
      "require": "./dist/cjs/types/index.js",
      "default": "./dist/esm/types/index.js"
    }
  },
  "scripts": {
    "prettier": "prettier . --write",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc -p tsconfig.json && rollup -c rollup.config.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@rolster/typescript-invertly": "^4.0.0",
    "@rolster/typescript-utils": "^4.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@rolster/typescript-types": "^4.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rolster-developments/typescript-coopplins.git"
  }
}
